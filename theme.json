                {
                    "slug": "accent-b-400",
                    "name": "Accent B 400 (Burgundy Shade 1)",
                    "color": "#7A3045"
                },
                {
                    "slug": "accent-b-500",
                    "name": "Accent B 500 (Core Burgundy)",
