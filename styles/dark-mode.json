{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"title": "Dark Mode",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#000000",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#2C2C2C",
					"name": "Neutral",
					"slug": "neutral"
				},
				{
					"color": "#B3B3B3",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#FF7847",
					"name": "Accent 1",
					"slug": "accent-1"
				},
				{
					"color": "#00D173",
					"name": "Accent 2",
					"slug": "accent-2"
				},
				{
					"color": "#66B2FF",
					"name": "Accent 3",
					"slug": "accent-3"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"bottom": "0.6rem",
								"left": "1.6rem",
								"right": "1.6rem",
								"top": "0.6rem"
							}
						}
					}
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|neutral",
					"text": "var:preset|color|base"
				},
				":hover": {
					"color": {
						"background": "color-mix(in srgb, var(--wp--preset--color--neutral) 85%, transparent)",
						"text": "var:preset|color|base"
					}
				},
				"spacing": {
					"padding": {
						"bottom": "0.6rem",
						"left": "1.6rem",
						"right": "1.6rem",
						"top": "0.6rem"
					}
				}
			}
		},
		"variations": {
			"section-2": {
				"elements": {
					"button": {
						"color": {
							"background": "var:preset|color|base",
							"text": "var:preset|color|neutral"
						},
						":hover": {
							"color": {
								"background": "color-mix(in srgb, var(--wp--preset--color--base) 85%, transparent)"
							}
						}
					}
				}
			},
			"section-4": {
				"elements": {
					"button": {
						"color": {
							"background": "var:preset|color|accent-2",
							"text": "var:preset|color|neutral"
						},
						":hover": {
							"color": {
								"background": "color-mix(in srgb, var(--wp--preset--color--accent-2) 85%, transparent)"
							}
						}
					}
				}
			}
		}
	}
}